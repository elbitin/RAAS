<?xml version="1.0"?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.8"/>
  </startup>
  <system.serviceModel>
    <client>
      <endpoint address="net.tcp://localhost:43000/RAASServerService"
        binding="netTcpBinding" bindingConfiguration="NetTcpBinding_IRAASServerService"
        contract="RAASServerServiceRef.IRAASServerService" name="NetTcpBinding_IRAASServerService">
        <identity>
          <servicePrincipalName value="host/SPECTRE" />
        </identity>
      </endpoint>
    </client>
    <services>
      <service name="Elbitin.Applications.RAAS.RAASClient.RAASClient.RAASClientService">
        <endpoint address="net.pipe://localhost/RAASClientService" binding="netNamedPipeBinding" bindingConfiguration="NetNamedPipeBinding_IRAASClientService" contract="Elbitin.Applications.RAAS.RAASClient.RAASClient.IRAASClientService"/>
        <endpoint address="net.pipe://localhost/RAASClientService/mex" binding="mexNamedPipeBinding" contract="IMetadataExchange"/>
        <host>
          <baseAddresses>
            <add baseAddress="net.pipe://localhost/RAASClientService"/>
          </baseAddresses>
        </host>
      </service>
    </services>
    <bindings>
      <netNamedPipeBinding>
        <binding name="NetNamedPipeBinding_IRAASClientService" receiveTimeout="00.00:10"
          sendTimeout="00.00:10" />
        <binding name="NetNamedPipeBinding_IRemoteAppsService" />
      </netNamedPipeBinding>
      <netTcpBinding>
        <binding name="NetTcpBinding_IRAASServerService">
          <reliableSession enabled="true" />
			<security>
				<transport sslProtocols="None" />
			</security>
        </binding>
      </netTcpBinding>
    </bindings>
      <behaviors>
      <serviceBehaviors>
        <behavior>
          <!-- To avoid disclosing metadata information, set the values below to false before deployment -->
          <serviceMetadata httpGetEnabled="false" httpsGetEnabled="false"/>
          <!-- To receive exception details in faults for debugging purposes, set the value below to true.  Set to false before deployment to avoid disclosing exception information -->
          <serviceDebug includeExceptionDetailInFaults="true"/>
        </behavior>
      </serviceBehaviors>
    </behaviors>
  </system.serviceModel>
</configuration>
