<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.8"/>
    </startup>
    <system.serviceModel>
        <client>
            <endpoint address="net.tcp://localhost:43000/RAASServerService" binding="netTcpBinding" bindingConfiguration="NetTcpBinding_IRAASServerService" contract="RAASServerServiceRef.IRAASServerService" name="NetTcpBinding_IRAASServerService">
            </endpoint>
        </client>
        <bindings>
            <netTcpBinding>
                <binding name="NetTcpBinding_IRAASServerService">
                    <reliableSession enabled="true"/>
					<security mode="Transport">
						<transport clientCredentialType="Windows"/>
					</security>
				</binding>
            </netTcpBinding>
        </bindings>
    </system.serviceModel>
</configuration>
